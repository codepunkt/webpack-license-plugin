{
  "name": "webpack-license-plugin",
  "version": "4.2.2",
  "description": "Extracts OSS license information of the npm packages in your webpack output",
  "author": "Christoph Werner <christoph@codepunkt.de>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codepunkt/webpack-license-plugin"
  },
  "keywords": [
    "webpack",
    "build",
    "artifact",
    "oss",
    "open source",
    "compliance",
    "copyright",
    "license",
    "licence",
    "licenses",
    "licences",
    "scan",
    "scanner",
    "bom",
    "bill of materials",
    "plugin"
  ],
  "main": "./dist/index.js",
  "contributors": [
    "Friedrich Wilms <f.wilms89@web.de>",
    "Tobias Trumm <info@tobiastrumm.de>",
    "Sebastian Peralta Friedburg <sebaspf@gmail.com>"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "clean": "node -e \"fs.rmSync('dist', { force: true, recursive: true })\"",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "build": "npm run lint && npm run clean && npm run typescript",
    "typescript": "tsc",
    "install:e2e": "cd test/e2e/example && npm i",
    "manual": "npm run build && webpack --config test/e2e/webpack.config.js",
    "prepublishOnly": "npm run build",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:e2e": "cross-env NODE_OPTIONS=--openssl-legacy-provider jest --runInBand --testPathPattern test/e2e",
    "test:unit": "jest --runInBand --testPathPattern test/unit --verbose",
    "test:watch": "jest --runInBand --testPathPattern test/unit --watch",
    "test:ci": "cross-env CI=true jest --runInBand --verbose --colors",
    "posttest:ci": "codecov -t $CODECOV_TOKEN"
  },
  "peerDependencies": {
    "webpack": ">=4.0.0 < 6.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "get-npm-tarball-url": "^2.0.3",
    "lodash": "^4.17.21",
    "needle": "^3.2.0",
    "spdx-expression-validate": "^2.0.0",
    "webpack-sources": "^3.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.41.0",
    "@types/jest": "29.5.5",
    "@types/lodash": "^4.14.198",
    "@types/memory-fs": "^0.3.3",
    "@types/needle": "^3.2.0",
    "@types/node": "^18.11.15",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "is-ci-cli": "^2.2.0",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^14.0.1",
    "memory-fs": "^0.5.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "file:./node_modules/webpack5",
    "webpack-cli": "^5.1.4",
    "webpack2": "npm:webpack@^2",
    "webpack3": "npm:webpack@^3",
    "webpack4": "npm:webpack@^4",
    "webpack5": "npm:webpack@5.88.2"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "git add"
    ]
  }
}
